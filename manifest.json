{
  "name": "facebook ext",
  "version": "1.0",
  "description": "uongsuadaubung",
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*"
      ],
      "js": [
        "util.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}